{
	"name": "aqua_sales",
	"version": "1.22.4",
	"description": "AquaSales",
	"main": "app.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/jesusrogliero/AquaSales.git"
	},
	"scripts": {
		"start": "electron .",
		"pack": "electron-builder --dir",
		"dist": "electron-builder",
		"publish": "dotenv -e .env -- electron-builder -p always"
	},
	"author": "Jesus Rogliero",
	"license": "ISC",
	"build": {
		"appId": "AquaSales",
		"productName": "AquaSales",
		"asar": true,
		"asarUnpack": [
			"**/node_modules/puppeteer-core/**/*",
			"**/node_modules/whatsapp-web.js/**/*"
		],
		"publish": [
			{
				"provider": "github",
				"owner": "jesusrogliero",
				"repo": "AquaSales"
			}
		],
		"win": {
			"target": [
				{
					"target": "nsis",
					"arch": [
						"x64"
					]
				}
			]
		}
	},
	"devDependencies": {
		"dotenv-cli": "^11.0.0",
		"electron": "latest",
		"electron-builder": "latest",
		"eslint": "latest",
		"eslint-plugin-react": "latest"
	},
	"dependencies": {
		"@navpreetdevpuri/html-pdf": "^3.0.9",
		"appdata-path": "^1.0.0",
		"axios": "latest",
		"bcrypt": "5.1.0",
		"electron-log": "latest",
		"electron-updater": "^4.0.0",
		"googleapis": "latest",
		"handlebars": "^4.7.8",
		"moment": "latest",
		"nodemailer": "latest",
		"qrcode": "^1.5.4",
		"qrcode-terminal": "^0.12.0",
		"sequelize": "^6.6.5",
		"sqlite3": "5.1.6",
		"whatsapp-web.js": "latest"
	}
}
